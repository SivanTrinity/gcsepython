import random
num = random.randint(1,5)
guess = int(input("enter number "))
if guess == num:
    print("well done")
elif guess >= num:
    print("too high")
    guess = int(input("enter number "))
    if guess != num:
        print("you lose")
elif guess <= num:
    print("too low")
    guess = int(input("enter number "))
    if guess != num:
        print("you lose")
